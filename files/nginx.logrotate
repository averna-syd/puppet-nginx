# This file is managed by Puppet.
# DO NOT edit this file directly.

/var/log/nginx/*.log {
  weekly
  missingok
  rotate 520
  compress
  delaycompress
  notifempty
  create 640 root adm
  sharedscripts
  postrotate
    [ ! -f /var/run/nginx.pid ] || kill -USR1 `cat /var/run/nginx.pid`
  endscript
}
